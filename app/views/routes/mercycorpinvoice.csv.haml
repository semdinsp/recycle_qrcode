- bc_headers = [  'name', 'suco',"collection_count","cost","report URL"]
= CSV.generate_line(bc_headers).strip.html_safe
- rtotal=0
- @bins.each do |bin|
  -count=0
  - collecturl="#{SETTINGS.host}#{SETTINGS.port}/bins/#{bin.id}/report"
  - dbscope=bin.actiontypes.thisMonth
  - dbscope=bin.actiontypes.lastMonth if @lastMonthFlag
  - count=dbscope.count  if !dbscope.empty?  #NEED TO FIX DATE
  - row=[bin.name, bin.suco, count, '%.2f' % [count*2,10].min, collecturl ]
  - rtotal = rtotal + [count*2,10].min
  = CSV.generate_line(row).strip.html_safe
- blankrow=["","","",""]
= CSV.generate_line(blankrow).strip.html_safe
- totalrow=["Total: ","" ,"" , '%.2f' % rtotal ]
= CSV.generate_line(totalrow).strip.html_safe
- dateRange=Time.now.all_month
- dateRange=Time.now.last_month.all_month if @lastMonthFlag
- daterow=["Date Range: ",dateRange.first,"", dateRange.last]
= CSV.generate_line(daterow).strip.html_safe
