- content_for :head do
  :javascript
    console.log("report html page ")

.row
  .col-3
    =image_tag "mercycorp.jpg",  size: "100x100"

  .col-3
    =image_tag "verde.jpg", size: "100x100"
.row
  %h2 This Month
  %table(class="table table-hover table-condensed table-responsive" width="100%" cellspacing="0" id="winners_table")
    %thead.thead-default
      %tr
        %th Name
        %th Suco
        %th Date
        -#%th User
        %th
        %th
    %tbody
      - @mybin.actiontypes.order("created_at DESC").each do |collection|
        %tr
          %td= collection.entity.name
          %td= collection.entity.suco
          %td
            = time_ago_in_words collection.created_at
            ago
          -# %td
          -#  = collection.user
          -#  (Coming soon)
.row
  %h5 Signature
  %p
.row
  %p _____________________
.row
  %p
    =@mybin.contactName
    ="[#{@mybin.telephone}]"
.row
  %h2 Past Month


.row


%p
  Links
  =link_to 'Routes', routemaps_bins_url
  |
  =link_to 'Collections', '/routes/collections'
