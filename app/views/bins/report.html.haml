- content_for :head do
  :javascript
    console.log("report html page ")

.row
  .col-1
  .col-3
    -#=image_tag "mercycorp.jpg",  size: "100x100"
  .col-2
  .col-3
    =image_tag "verde.jpg", size: "100x100"
.row
  .col-1
  .col
    %h4
      Date:
      = Time.now.to_date
    %table(class="table table-hover table-condensed table-responsive" width="100%" cellspacing="0" id="winners_table")
      %thead.thead-default
        %tr
          %th Name
          %th Suco
          %th Collection Date
          %th Time
          -#%th User
          %th
          %th
      %tbody
        - @mybin.actiontypes.order("created_at DESC").each do |collection|
          %tr
            %td= collection.entity.name
            %td= collection.entity.suco
            %td
              = collection.created_at.to_date
            %td
              = collection.created_at.strftime("%H:%M:%S")
            -# %td
            -#  = collection.user
            -#  (Coming soon)
.row
  .col-1
  .col
    %h5 Customer Signature
    %p
.row
  .col-1
  .col
    %p _____________________
.row
  .col-1
  .col
    %p
      =@mybin.contactName
      ="[#{@mybin.telephone}]"

.row
  .col-1
  .col
    %h2 Past Month - TBC


.row

.row
  .col-1
  .col
.row
  .col-1
  .col
    %p
      Links
      =link_to 'Routes', routemaps_bins_url
      |
      =link_to 'Collections', '/routes/collections'
