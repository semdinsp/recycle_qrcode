%table(class="table table-hover table-condensed table-responsive" width="100%" cellspacing="0" id="winners_table")
  %thead.thead-default
    %tr
      %th Entity
      %th Suco
      %th Time
      %th Latitude/Longitude
      %th Accuracy
      %th To Bin (km)
      %th Time UTC
      %th
  %tbody
    - locals[:clist].each do |collection|
      %tr
        %td= collection.entity.name
        %td= collection.entity.suco
        %td
          = time_ago_in_words collection.created_at
          ago
        %td= collection.location.latLngShort if !collection.location.nil?
        %td= number_with_precision(collection.location.accuracy, :precision => 2)  if !collection.location.nil?
        %td= number_with_precision(collection.location.haversine_distance(collection.entity.location), :precision => 2)  if !collection.location.nil?
        %td= collection.created_at.strftime('%d %b %Y %H:%M:%S')
        %td= link_to 'Report', bin_report_url(collection.entity)
